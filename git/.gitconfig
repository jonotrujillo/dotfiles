[core]
  editor = vim
  excludesfile = ~/.gitignore
[credential]
  helper = osxkeychain
[pull]
  rebase = true
[push]
  default = simple
[filter "lfs"]
  clean = git-lfs clean -- %f
  process = git-lfs filter-process
  required = true
  smudge = git-lfs smudge -- %f
[init]
  defaultBranch = main
[user]
  email = 313951+jonotrujillo@users.noreply.github.com
  name = Jonathan Trujillo
[include]
  path = ~/.gitconfig.local
